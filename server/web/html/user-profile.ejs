<%- include('./static/header') %>

<div class="jumbotron bg-indigo">
 <center> <img src="<%=avatar%>" alt="Greesy | <%=ismi%>#<%=disc%>" /></center>
  <br>
  <center>
    <% if(owner){ %>
    <i class="fad fa-crown" id="owner"></i>
    <%}%>
<% if(Mod){ %>
  
  <i class="fad fa-wrench" id="mod"></i>
  
  <%}%>
    <% rozetler.forEach(flag=> { %>
  <%  if (flag === "HOUSE_BALANCE") { %>
                                    <span data-tooltip="Hypesquad Balance"
                                    data-tooltip-location="top">
                                    <img draggable="false" src="https://discord.com/assets/9fdc63ef8a3cc1617c7586286c34e4f1.svg" data-tooltip="Hypesquad Balance" class="tippy mr-1" data-tippy-content="Hypesquad Balance" width="32px">
                                </span>
                                    <% } %>
                
    <% }) %>
   </center>

</div>

<script src="https://unpkg.com/@popperjs/core@2/dist/umd/popper.min.js"></script>
<script src="https://unpkg.com/tippy.js@6/dist/tippy-bundle.umd.js"></script>
<script>
      // With the above scripts loaded, you can call `tippy()` with a CSS
      // selector and a `content` prop:
      tippy('#mod', {
        content: 'Greesy Moderator',
      });
  tippy('#owner', {
        content: 'Greesy Owner',
      });
    
    </script>
